@model IEnumerable<GodelTourAgensy.Models.TravelPackageViewModel>

@{
    ViewData["Title"] = "Результаты";
}

<label class="text-center form-control m-1">Результаты:</label>
<table class="table table-striped table-hover table-bordered m-1">
    <thead class="thead-dark">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ClientId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RoomId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BookingMark)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StartDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EndDate)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(model => item.Id)
                </td>
                <td>
                    @Html.DisplayFor(model => item.ClientId)
                </td>
               
                <td>
                    @Html.DisplayFor(model => item.RoomId)
                </td>
                <td>
                    @Html.DisplayFor(model => item.BookingMark)
                </td>
                <td>
                    @Html.DisplayFor(model => item.StartDate)
                </td>
                <td>
                    @Html.DisplayFor(model => item.EndDate)
                </td>
            </tr>
        }
    </tbody>
</table>