@model IEnumerable<(int Name,int Count)>

<div class="container">
    <label class="text-center form-control m-1">Результаты:</label>
    <table class="table table-striped table-hover table-bordered m-1">
        <thead class="thead-dark">
            <tr>
                <th>
                    Номер
                </th>
                <th>
                    Количество
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(model => item.Name)
                    </td>
                    <td>
                        @Html.DisplayFor(model => item.Count)
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>